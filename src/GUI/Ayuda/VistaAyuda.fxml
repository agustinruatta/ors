<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Ayuda.PresentadorAyuda">
   <bottom>
      <Button mnemonicParsing="false" onAction="#volverAlMenuPrincipal" text="%general.volver_menu_principal" BorderPane.alignment="CENTER_LEFT">
         <BorderPane.margin>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
   <left>
      <TreeView fx:id="menuLateralAyuda" prefHeight="200.0" prefWidth="400.0" BorderPane.alignment="CENTER" />
   </left>
   <center>
      <TextArea fx:id="descripcionAyuda" editable="false" prefHeight="200.0" prefWidth="200.0" wrapText="true" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
